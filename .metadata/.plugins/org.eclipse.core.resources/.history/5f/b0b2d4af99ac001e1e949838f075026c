import javax.swing.*;
import java.awt.*;

public class UnClickableButtonExample {
    public static void main(String[] args) {
        SwingUtilities.invokeLater(() -> createAndShowGUI());
    }

    private static void createAndShowGUI() {
        JFrame frame = new JFrame("UnClickable Button Example");
        frame.setDefaultCloseOperation(JFrame.EXIT_ON_CLOSE);

        // Load your image (replace "your_image_path.jpg" with the actual path to your image)
        ImageIcon imageIcon = new ImageIcon("your_image_path.jpg");

        // Create a JButton and set the ImageIcon as its icon
        JButton unclickableButton = new JButton(imageIcon);

        // Disable the button to make it unclickable
        unclickableButton.setEnabled(false);

        // Set the layout manager for the frame
        frame.setLayout(new FlowLayout());

        // Add the button to the frame
        frame.add(unclickableButton);

        // Adjust frame properties
        frame.setSize(300, 200);
        frame.setLocationRelativeTo(null);
        frame.setVisible(true);
    }
}
